{
  "manifest_version": 2,

  "name": "html",
  "description": "inside the world",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": [ "http://*/*", "https://*/*"]
    }
  ],

  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "webRequest"
  ]
}